<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <!-- Sequences for all seq in underwriting section-->
         <record id="helpdesk_ticket_number" model="ir.sequence">
            <field name="name">Helpdesk No</field>
            <field name="code">helpdesk_ticket.seq</field>
            <field name="prefix">TICKER/%(year)s/%(month)s/</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>

    </data>
</odoo>
